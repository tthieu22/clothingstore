{
    "version": 2,
    "functions": {
        "*.php": {
            "runtime": "vercel-php@0.3.6"
        }
    },
    "builds": [
        {
            "src": "*.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}